GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/BlocksKit" "${PODS_ROOT}/Headers/Public/GGComponent" "${PODS_ROOT}/Headers/Public/GGLogining" "${PODS_ROOT}/Headers/Public/KVOController"
LIBRARY_SEARCH_PATHS = $(inherited) "${PODS_CONFIGURATION_BUILD_DIR}/BlocksKit" "${PODS_CONFIGURATION_BUILD_DIR}/GGComponent" "${PODS_CONFIGURATION_BUILD_DIR}/GGLogining" "${PODS_CONFIGURATION_BUILD_DIR}/KVOController"
OTHER_LDFLAGS = $(inherited) -ObjC -l"BlocksKit" -l"GGComponent" -l"GGLogining" -l"KVOController" -framework "MessageUI"
PODS_BUILD_DIR = ${BUILD_DIR}
PODS_CONFIGURATION_BUILD_DIR = ${PODS_BUILD_DIR}/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_PODFILE_DIR_PATH = ${SRCROOT}/.
PODS_ROOT = ${SRCROOT}/Pods
